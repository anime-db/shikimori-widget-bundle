{
    "name": "anime-db/shikimori-widget-bundle",
    "license": "GPL-3.0",
    "type": "library",
    "description": "Plugin AniDB for the catalog Anime DB",
    "homepage": "http://github.com/anime-db/shikimori-widget-bundle",
    "version": "0.1.0",
    "autoload": {
        "psr-4": {
            "AnimeDb\\Bundle\\ShikimoriWidgetBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AnimeDb\\Bundle\\ShikimoriWidgetBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.4.0",
        "anime-db/catalog-bundle": "dev-develop",
        "anime-db/shikimori-browser-bundle": "0.1.*"
    },
    "require-dev": {
        "anime-db/shikimori-filler-bundle": "0.1.*"
    },
    "suggest": {
        "anime-db/shikimori-filler-bundle": "Allows to add links to the filling items"
    },
    "minimum-stability": "dev"
}
